@model IEnumerable<SportAssociation.Models.Ticket_Buying_Transactions>

@{
    ViewData["Title"] = "Index";
}

@if (TempData["alertMessage"] != null)
{
        <script type="text/javascript">
            alert('@TempData["alertMessage"]');
        </script>
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.fan_id)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.fan_id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ticket_id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ticket_id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ticket_id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
